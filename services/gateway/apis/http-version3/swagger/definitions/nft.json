{
    "NFTAttributes": {
        "type": "object",
        "required": [
            "module",
            "attributes"
        ],
        "properties": {
            "module": {
                "type": "string",
                "example": "customModule"
            },
            "attribute": {
                "type": "string",
                "example": "Custom attribute"
            }
        }
    },
    "NFT": {
        "type": "object",
        "required": [
            "id",
            "nft",
            "owner",
            "attributes",
            "lockingModule"
        ],
        "properties": {
            "id": {
                "type": "string",
                "description": "Universal identifier of the NFT.",
                "example": "00000000100000001"
            },
            "nft": {
                "type": "object",
                "required": [
                    "chainID",
                    "collectionID",
                    "index"
                ],
                "properties": {
                    "chainID": {
                        "type": "string",
                        "description": "Chain ID on which the NFT was created.",
                        "example": "00000000"
                    },
                    "collectionID": {
                        "type": "string",
                        "description": "Collection ID of the NFT.",
                        "example": "10000000"
                    },
                    "index": {
                        "type": "number",
                        "description": "Index of NFT within the collection.",
                        "example": 1
                    }
                }
            },
            "owner": {
                "type": "string",
                "description": "Owner of the NFT. It will be an address when owned by an account on the chain it was created or a chainID when escrowed.",
                "example": "lskyvvam5rxyvbvofxbdfcupxetzmqxu22phm4yuo"
            },
            "attributes": {
                "description": "List of NFT attributes.",
                "type": "array",
                "items": {
                    "$ref": "#/definitions/NFTAttributes"
                }
            },
            "lockingModule": {
                "type": "string",
                "description": "Module name that locked the NFT.",
                "example": "customModule"
            },
            "escrowedChainID": {
                "type": "string",
                "description": "Chain ID to which the NFT is escrowed.",
                "example": "04000002"
            }
        }
    },
    "NFTEscrowed": {
        "type": "object",
        "required": [
            "escrowedChainID",
            "id",
            "nft"
        ],
        "properties": {
            "escrowedChainID": {
                "type": "string",
                "description": "Chain ID to which the NFT is escrowed.",
                "example": "04000002"
            },
            "id": {
                "type": "string",
                "description": "Universal identifier of the NFT.",
                "example": "00000000100000001"
            },
            "nft": {
                "type": "object",
                "required": [
                    "chainID",
                    "collectionID",
                    "index"
                ],
                "properties": {
                    "chainID": {
                        "type": "string",
                        "description": "Chain ID on which the NFT was created.",
                        "example": "00000000"
                    },
                    "collectionID": {
                        "type": "string",
                        "description": "Collection ID of the NFT.",
                        "example": "10000000"
                    },
                    "index": {
                        "type": "number",
                        "description": "Index of NFT within the collection.",
                        "example": 1
                    }
                }
            }
        }
    },
    "NFTsWithEnvelope": {
        "type": "object",
        "required": [
            "data",
            "meta"
        ],
        "properties": {
            "data": {
                "description": "List of NFTs.",
                "type": "array",
                "items": {
                    "$ref": "#/definitions/NFT"
                }
            },
            "meta": {
                "$ref": "#/definitions/pagination"
            }
        }
    },
    "NFTEscrowedWithEnvelope": {
        "type": "object",
        "required": [
            "data",
            "meta"
        ],
        "properties": {
            "data": {
                "description": "List of NFTs.",
                "type": "array",
                "items": {
                    "$ref": "#/definitions/NFTEscrowed"
                }
            },
            "meta": {}
        }
    },
    "nftConstantsWithEnvelope": {
        "type": "object",
        "required": [
            "data",
            "meta"
        ],
        "properties": {
            "data": {
                "description": "NFT module constants.",
                "type": "object",
                "required": [
                    "feeCreateNFT"
                ],
                "properties": {
                    "feeCreateNFT": {
                        "type": "string",
                        "example": "5000000",
                        "description": "The extra fee to be supplied for creating NFT."
                    }
                }
            },
            "meta": {
                "type": "object"
            }
        }
    },
    "NFTHistory": {
        "type": "object",
        "required": [
            "transactionID",
            "block"
        ],
        "properties": {
            "owner": {
                "type": "object",
                "required": [
                    "new",
                    "old"
                ],
                "properties": {
                    "new": {
                        "type": "string",
                        "description": "New owner of the NFT.",
                        "example": "lskyvvam5rxyvbvofxbdfcupxetzmqxu22phm4yuo"
                    },
                    "old": {
                        "type": "string",
                        "description": "Old owner of the NFT.",
                        "example": "lskguo9kqnea2zsfo3a6qppozsxsg92nuuma3p7ad"
                    }
                }
            },
            "transactionID": {
                "type": "string",
                "description": "Unique identifier of the transaction.\nDerived from the transaction signature.",
                "example": "f9593f101c4acafc3ede650ab4c10fa2ecb59b225813eddbbb17b47e96932e9b"
            },
            "block": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "id",
                        "minLength": 1,
                        "maxLength": 64,
                        "example": "01967dba384998026fe028119bd099ecf073c05c045381500a93d1a7c7307e5b",
                        "description": "Unique identifier of the block.\nDerived from the block signature."
                    },
                    "height": {
                        "type": "integer",
                        "minimum": 1,
                        "example": 8350681,
                        "description": "The height of the network, at the moment where this transaction was included in the blockchain."
                    },
                    "timestamp": {
                        "type": "integer",
                        "example": 1613323667,
                        "description": "UNIX Timestamp"
                    }
                }
            }
        }
    },
    "NFTHistoryWithEnvelope": {
        "type": "object",
        "required": [
            "data",
            "meta"
        ],
        "properties": {
            "data": {
                "description": "NFT history by type. Based on type, only one of 'transfer' or 'attribute' entries will be present in the response",
                "type": "array",
                "items": {
                    "$ref": "#/definitions/NFTHistory"
                }
            },
            "meta": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "Universal identifier of the NFT.",
                        "example": "0000000010000000000000000000000000"
                    },
                    "nft": {
                        "type": "object",
                        "required": [
                            "chainID",
                            "collectionID",
                            "index"
                        ],
                        "properties": {
                            "chainID": {
                                "type": "string",
                                "description": "Chain ID on which the NFT was created.",
                                "example": "00000000"
                            },
                            "collectionID": {
                                "type": "string",
                                "description": "Collection ID of the NFT.",
                                "example": "10000000"
                            },
                            "index": {
                                "type": "number",
                                "description": "Index of NFT within the collection.",
                                "example": 1
                            }
                        }
                    }
                }
            }
        }
    },
    "nftSupportedWithEnvelope": {
        "type": "object",
        "required": [
            "data",
            "meta"
        ],
        "properties": {
            "data": {
                "description": "NFT module supported collection IDs information.",
                "type": "object",
                "required": [
                    "isSupportAllNFTs",
                    "patternCollectionIDs",
                    "exactCollectionIDs"
                ],
                "properties": {
                    "isSupportAllNFTs": {
                        "type": "boolean",
                        "example": "false",
                        "description": "A boolean indicating if all NFTs are supported by the application."
                    },
                    "patternCollectionIDs": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "description": "A pattern representing chain IDs whose all collection IDs are supported by the application."
                        },
                        "example": "[00000000********, 00000001********]"
                    },
                    "exactCollectionIDs": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "description": "List of NFT collection IDs supported by the application."
                        },
                        "example": "[0000000210000000, 0000000220000000]"
                    }
                }
            },
            "meta": {
                "type": "object"
            }
        }
    }
}
