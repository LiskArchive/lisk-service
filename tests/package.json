    "artillery": "^1.7.8",
    "await-to-js": "=2.1.1",
    "axios": "^0.21.2",
    "babel-jest": "^24.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-lisk-base": "^0.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "=22.21.0",
    "eslint-plugin-mocha": "^4.12.1",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "joi": "^17.4.0",
    "moment": "=2.24.0"
