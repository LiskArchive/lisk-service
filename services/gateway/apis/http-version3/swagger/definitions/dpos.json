{
    "delegate": {
        "type": "object",
        "required": [
            "name",
            "address",
            "totalVotesReceived",
            "selfVotes",
            "voteWeight",
            "status",
            "isBanned",
            "pomHeights"
        ],
        "properties": {
            "name": {
                "type": "string",
                "example": "genesis_84",
                "description": "Delegate name"
            },
            "totalVotesReceived": {
                "type": "string",
                "example": "109000000000",
                "description": "Total votes received"
            },
            "selfVotes": {
                "type": "string",
                "example": "109000000000",
                "description": "Self votes"
            },
            "voteWeight": {
                "type": "string",
                "example": "109000000000",
                "description": "vote weight"
            },
            "address": {
                "type": "string",
                "format": "address",
                "example": "lskdwsyfmcko6mcd357446yatromr9vzgu7eb8y99",
                "description": "The Lisk Address is the human-readable representation of a blockchain account.\nIt is 41 character long identifier that begins with `lsk`.\n"
            },
            "lastGeneratedHeight": {
                "type": "integer",
                "example": 0
            },
            "status": {
                "type": "string",
                "example": "active",
                "description": "Delegate status"
            },
            "isBanned": {
                "type": "boolean",
                "example": "false"
            },
            "pomHeights": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "start": {
                            "type": "integer",
                            "example": 123
                        },
                        "end": {
                            "type": "integer",
                            "example": 567
                        }
                    }
                }
            },
            "consecutiveMissedBlocks": {
                "type": "integer",
                "example": 0
            },
            "forgedBlocks": {
                "type": "integer",
                "example": 1000,
                "description": "Number of blocks forged by the delegate."
            },
            "rank": {
                "type": "integer",
                "example": 93,
                "description": "Delegate rank."
            },
            "rewards": {
                "type": "string",
                "example": "109000000000",
                "description": "Rewards earned by the delegate for forging blocks in terms of the current chain's native token, e.g.: LSK on the mainchain."
            }
        }
    },
    "delegatesWithEnvelope": {
        "type": "object",
        "required": [
            "data",
            "meta"
        ],
        "properties": {
            "data": {
                "description": "List of delegates",
                "type": "array",
                "items": {
                    "$ref": "#/definitions/delegate"
                }
            },
            "meta": {
                "$ref": "#/definitions/Pagination"
            }
        }
    },
    "unlock": {
        "type": "object",
        "required": [
            "address",
            "publicKey",
            "name"
        ],
        "properties": {
            "address": {
                "type": "string",
                "format": "address",
                "example": "lskdwsyfmcko6mcd357446yatromr9vzgu7eb8y99",
                "description": "The Lisk Address is the human-readable representation of a blockchain account.\nIt is 41 character long identifier that begins with `lsk`.\n"
            },
            "publicKey": {
                "type": "string",
                "format": "publicKey",
                "example": "b1d6bc6c7edd0673f5fed0681b73de6eb70539c21278b300f07ade277e1962cd",
                "description": "The public key is derived from the private key of the owner of the account.\nIt can be used to validate that the private key belongs to the owner, but not provide access to the owners private key.\n"
            },
            "name": {
                "type": "string",
                "example": "genesis_84",
                "description": "Name of auth account"
            },
            "unlocking": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "delegateAddress": {
                            "type": "string",
                            "example": "lsk24cd35u4jdq8szo3pnsqe5dsxwrnazyqqqg5eu"
                        },
                        "amount": {
                            "type": "string",
                            "example": "10000"
                        },
                        "unvoteHeight": {
                            "type": "object",
                            "properties": {
                                "start": {
                                    "type": "integer",
                                    "example": 30
                                },
                                "end": {
                                    "type": "integer",
                                    "example": 300
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "unlocksWithEnvelope": {
        "type": "object",
        "required": [
            "data",
            "meta"
        ],
        "properties": {
            "data": {
                "description": "Array of delegate unlocks",
                "type": "array",
                "items": {
                    "$ref": "#/definitions/unlock"
                }
            },
            "meta": {
                "$ref": "#/definitions/Pagination"
            }
        }
    },
    "Vote": {
        "type": "object",
        "required": [
            "delegateAddress",
            "amount",
            "name",
            "voteWeight",
            "rank"
        ],
        "properties": {
            "delegateAddress": {
                "type": "string",
                "format": "address",
                "example": "lskdwsyfmcko6mcd357446yatromr9vzgu7eb8y99",
                "description": "Lisk Address of the delegate the queried account voted for."
            },
            "amount": {
                "type": "string",
                "example": 1081560729258
            },
            "name": {
                "type": "string",
                "format": "username",
                "example": "liskhq",
                "description": "Name of the delegate the queried account voted for."
            },
            "voteWeight": {
                "type": "string",
                "example": "109000000000",
                "description": "Current delegate's vote weight."
            },
            "rank": {
                "type": "integer",
                "example": 93,
                "description": "Current delegate's rank."
            }
        }
    },
    "VotesReceivedWithEnvelope": {
        "type": "object",
        "required": [
            "data",
            "meta"
        ],
        "properties": {
            "data": {
                "description": "List of votes",
                "type": "object",
                "properties": {
                    "account": {
                        "type": "object",
                        "properties": {
                            "address": {
                                "type": "string",
                                "format": "address",
                                "example": "lskdwsyfmcko6mcd357446yatromr9vzgu7eb8y99",
                                "description": "The Lisk Address is the human-readable representation of a blockchain account.\nIt is 41 character long identifier that begins with `lsk`.\n"
                            },
                            "name": {
                                "type": "string",
                                "example": "genesis_84",
                                "description": "Delegate name"
                            },
                            "publicKey": {
                                "type": "string",
                                "format": "publicKey",
                                "example": "b1d6bc6c7edd0673f5fed0681b73de6eb70539c21278b300f07ade277e1962cd",
                                "description": "The public key is derived from the private key of the owner of the account.\nIt can be used to validate that the private key belongs to the owner, but not provide access to the owners private key.\n"
                            }
                        }
                    },
                    "votes": {
                        "type": "array",
                        "items": {
                            "$ref": "#/definitions/Vote"
                        }
                    }
                }
            },
            "meta": {
                "$ref": "#/definitions/Pagination"
            }
        }
    },
    "VotesSentWithEnvelope": {
        "type": "object",
        "required": [
            "data",
            "meta"
        ],
        "properties": {
            "data": {
                "description": "List of votes",
                "type": "object",
                "properties": {
                    "account": {
                        "type": "object",
                        "properties": {
                            "address": {
                                "type": "string",
                                "format": "address",
                                "example": "lskdwsyfmcko6mcd357446yatromr9vzgu7eb8y99",
                                "description": "The Lisk Address is the human-readable representation of a blockchain account.\nIt is 41 character long identifier that begins with `lsk`.\n"
                            },
                            "name": {
                                "type": "string",
                                "example": "genesis_84",
                                "description": "Delegate name"
                            },
                            "publicKey": {
                                "type": "string",
                                "format": "publicKey",
                                "example": "b1d6bc6c7edd0673f5fed0681b73de6eb70539c21278b300f07ade277e1962cd",
                                "description": "The public key is derived from the private key of the owner of the account.\nIt can be used to validate that the private key belongs to the owner, but not provide access to the owners private key.\n"
                            },
                            "votesUsed": {
                                "type": "integer",
                                "example": 10,
                                "description": ""
                            }
                        }
                    },
                    "votes": {
                        "type": "array",
                        "items": {
                            "$ref": "#/definitions/Vote"
                        }
                    }
                }
            },
            "meta": {
                "$ref": "#/definitions/Pagination"
            }
        }
    },
    "DPoSConstantsWithEnvelope": {
        "type": "object",
        "required": [
            "data",
            "meta"
        ],
        "properties": {
            "data": {
                "description": "DPoS module constants",
                "type": "object",
                "properties": {
                    "factorSelfVotes": {
                        "type": "integer",
                        "example": 10,
                        "description": "The factor multiplying the self-votes of a delegate for the delegate weight computation. It is used to set the ceiling for the delegate weight."
                    },
                    "maxLengthName": {
                        "type": "integer",
                        "example": 20,
                        "description": "The maximum number of characters allowed for a delegate name."
                    },
                    "maxNumberSentVotes": {
                        "type": "integer",
                        "example": 10,
                        "description": "The maximum number of delegates any voter can vote for."
                    },
                    "maxNumberPendingUnlocks": {
                        "type": "integer",
                        "example": 20,
                        "description": "The maximum number of pending unlocks allowed for a voter."
                    },
                    "failSafeMissedBlocks": {
                        "type": "integer",
                        "example": 50,
                        "description": "The number of consecutive missed blocks used in the fail-safe banning mechanism."
                    },
                    "failSafeInactiveWindow": {
                        "type": "integer",
                        "example": 260000,
                        "description": "The length of the inactivity window used in the fail-safe banning mechanism."
                    },
                    "punishmentWindow": {
                        "type": "integer",
                        "example": 780000,
                        "description": "The punishment time for punished delegates."
                    },
                    "roundLength": {
                        "type": "integer",
                        "example": 10,
                        "description": "The number of blocks in a round and is the sum total of the active and the standby delegates."
                    },
                    "bftThreshold": {
                        "type": "integer",
                        "example": 68,
                        "description": "The pre-commit and certificate thresholds used by the BFT module."
                    },
                    "minWeightStandby": {
                        "type": "string",
                        "example": "100000000000",
                        "description": "The minimum delegate weight required to be eligible as a standby delegate."
                    },
                    "numberActiveDelegates": {
                        "type": "integer",
                        "example": 101,
                        "description": "The number of active delegates in a round."
                    },
                    "numberStandbyDelegates": {
                        "type": "integer",
                        "example": 2,
                        "description": "The number of standby delegates in a round."
                    },
                    "tokenIDDPoS": {
                        "type": "string",
                        "example": "0000000000000000",
                        "description": "The universal identifier for the token within the Lisk ecosystem for DPoS module. It is defined as the concatenated value of the chainID and localID. Example: tokenID for LSK would be '0000000100000000' (chainID: '00000001', localID: '00000000')"
                    }
                }
            },
            "meta": {
                "type": "object"
            }
        }
    }
}
