    "log4js": "^5.3.0",
    "method-override": "^3.0.0",
    "moleculer": "=0.13.12",
    "moment": "=2.24.0",
    "nats": "=1.3.2",
    "pg-promise": "=9.2.1",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.7",
    "require-all": "^3.0.0",
    "socket.io": "^2.2.0",
