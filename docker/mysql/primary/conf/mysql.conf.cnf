[mysqld]

# skip-name-resolve
default_authentication_plugin = mysql_native_password

server_id = 1
log_bin=mysql-bin.log
# binlog_format = ROW
# binlog_do_db = lisk
gtid_mode=ON
enforce_gtid_consistency=ON



# suggested -> log_bin=/var/log/mysql/mysql-bin.log
