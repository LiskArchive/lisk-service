  "main": "app.js",
  "engines": {
    "node": ">=16.14.0 <=16",
    "npm": ">=8.3.1"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules",
    "test": "npm run test:integration:APIv2:SDKv5",
    "test:integration:APIv2:SDKv5": "jest --config=jest.config.integration.apiv2.sdkv5.js",
    "test:integration:APIv3:SDKv6": "echo integration tests",
    "test:functional": "jest --config=jest.config.functional.js",
    "test:regression": "jest --config=jest.config.regression.js",
    "benchmark:RPC:APIv2": "artillery run benchmark/socket_rpc_v2.yaml",
    "benchmark:HTTP:APIv2": "artillery run benchmark/http_v2.yaml",
    "benchmark": "npm run benchmark:RPC:APIv2 && npm run benchmark:HTTP:APIv2",
    "start": "node app.js"
  },
  "dependencies": {
    "async": "^3.2.3",
    "chai": "^4.3.4",
    "csv-parse": "^5.0.3",
    "express": "^4.17.1",
    "http-proxy": "^1.18.1",
    "http-status-codes": "^1.4.0",
    "jest-watch-toggle-config": "^1.0.2",
    "jest-watch-typeahead": "^0.4.2",
    "jsome": "^2.5.0",
    "log4js": "^6.4.0",
    "mockserver": "^3.1.1",
    "multimatch": "^3.0.0",
    "popsicle": "^10.0.1",
    "prettyjson": "^1.2.1",
    "semver": "^7.3.5",
    "socket.io": "^4.1.0",
    "socket.io-client": "^4.1.0",
