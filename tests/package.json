{
  "name": "lisk-service-tests",
  "version": "0.1.0",
  "description": "Lisk Service Test Package",
  "keywords": [
    "lisk",
    "service",
    "blockchain"
  ],
  "homepage": "https://github.com/LiskHQ/lisk-service",
  "repository": {
    "type": "git",
    "url": "git@github.com:LiskHQ/lisk-service.git"
  },
  "bugs": {
    "url": "https://github.com/LiskHQ/lisk-service/issues"
  },
  "private": true,
  "author": "Lisk Foundation <admin@lisk.io>, lightcurve GmbH <admin@lightcurve.io>",
  "license": "GPL-3.0",
  "main": "app.js",
  "engines": {
    "node": "10.19.0",
    "npm": "6.13.4"
  },
  "scripts": {
    "eslint": "eslint ./integration ./*.js",
    "eslint-fix": "eslint --fix ./integration ./*.js",
    "clean": "rm -rf ./node_modules",
    "test": "npm run test:integration",
    "test:testnet": "npm run test:integration:v1:testnet",
    "test:bitcoin:mock": "jest --config=config/jest.mock.bitcoin.config.js",
    "test:integration": "npm run test:integration:legacy && npm run test:integration:v1",
    "test:integration:legacy": "npm run test:integration:legacy:local",
    "test:integration:legacy:local": "jest --testTimeout=15000 --config=integration/jest.http.legacy.local.config.js",
    "test:integration:legacy:testnet": "jest --config=integration/jest.http.legacy.testnet.config.js",
    "test:integration:legacy:external": "jest --config=integration/jest.http.legacy.external.config.js",
    "test:integration:v1": "jest --testTimeout=15000 --config=integration/jest.v1.config.js",
    "test:integration:v1:jenkins": "npm run test:integration:v1 -- --runInBand",
    "test:integration:v1:local": "npm run test:integration:v1 -- v1_loacal",
    "test:integration:v1:testnet": "npm run test:integration:v1 -- v1_testnet",
    "test:integration:v1:socketRpc": "npm run test:integration:v1 -- socketJsonRpc_v1",
    "test:integration:v1:socketRpc:local": "npm run test:integration:v1 -- socketJsonRpc_v1_local",
    "test:integration:v1:socketRpc:testnet": "npm run test:integration:v1 -- socketJsonRpc_v1_testnet",
    "test:integration:v1:http": "npm run test:integration:v1 -- http_v1",
    "test:integration:v1:http:local": "npm run test:integration:v1 -- http_v1_local",
    "test:integration:v1:http:testnet": "npm run test:integration:v1 -- http_v1_testnet",
    "start": "node app.js"
  },
  "dependencies": {
    "@liskhq/lisk-cryptography": "^2.2.0",
    "chai": "^4.2.0",
    "express": "^4.17.1",
    "http-proxy": "^1.18.0",
    "http-status-codes": "^1.3.1",
    "jest-watch-toggle-config": "^1.0.2",
    "jest-watch-typeahead": "^0.4.2",
    "jsome": "^2.5.0",
    "log4js": "^4.3.1",
    "mockserver": "^3.0.0",
    "multimatch": "^3.0.0",
    "popsicle": "^10.0.1",
    "prettyjson": "^1.2.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "vscode-json-languageservice": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@hapi/joi": "^17.1.0",
    "await-to-js": "=2.1.1",
    "axios": "^0.19.0",
    "babel-jest": "^24.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-lisk-base": "^0.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "=22.21.0",
    "eslint-plugin-mocha": "^4.11.0",
    "jest": "^25.1.0",
    "jest-extended": "^0.11.2",
    "mocha": "^5.2.0",
    "moment": "=2.24.0",
    "supertest": "^3.4.2"
  }
}
